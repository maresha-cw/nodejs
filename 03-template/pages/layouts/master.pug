                                            a(href='#')
                                                .notif-img
                                                    img(src='/img/chadengle.jpg' alt='Img Profile')
                                                .notif-content
                                                    span.subject Chad
                                                    span.block  Ok, Thanks !
                                                    span.time 12 minutes ago
                                            a(href='#')
                                                .notif-img
                                                    img(src='/img/mlane.jpg' alt='Img Profile')
                                                .notif-content
                                                    span.subject Jhon Doe
                                                    span.block
                                                        | Ready for the meeting today...
                                                    span.time 12 minutes ago
                                            a(href='#')
                                                .notif-img
                                                    img(src='/img/talha.jpg' alt='Img Profile')
                                                .notif-content
                                                    span.subject Talha
                                                    span.block  Hi, Apa Kabar ?
                                                    span.time 17 minutes ago
                                li
                                    a.see-all(href='javascript:void(0);')
