                                        i.fa.fa-angle-right
                        li.nav-item.topbar-icon.dropdown.hidden-caret
                            a#notifDropdown.nav-link.dropdown-toggle(href='#' role='button' data-bs-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                i.fa.fa-bell
                                span.notification 4
                            ul.dropdown-menu.notif-box.animated.fadeIn(aria-labelledby='notifDropdown')
                                li
                                    .dropdown-title
                                        | You have 4 new notification
                                li
                                    .notif-scroll.scrollbar-outer
                                        .notif-center
                                            a(href='#')
                                                .notif-icon.notif-primary
                                                    i.fa.fa-user-plus
                                                .notif-content
                                                    span.block  New user registered
                                                    span.time 5 minutes ago
                                            a(href='#')
                                                .notif-icon.notif-success
                                                    i.fa.fa-comment
                                                .notif-content
                                                    span.block
                                                        | Rahmad commented on Admin
                                                    span.time 12 minutes ago
